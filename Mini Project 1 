        "%%sql\n",
        "SELECT city, COUNT(name) AS \"Number of Cafes\", SUM(review_count) AS \"Number of Reviews\", state\n",
        "FROM Business\n",
        "WHERE categories LIKE \"%Cafes%\" AND is_open = 1 AND state = \"LA\"\n",
        "GROUP BY city \n",
        "ORDER BY COUNT(name) DESC, review_count DESC \n",
        "LIMIT 10;"
      ]
